<?xml version="1.0" encoding="UTF-8"?>

<VODML
	xmlns="http://www.ivoa.net/xml/mivot">

	<MODEL name="model" />
	<GLOBALS >
	
		<COLLECTION dmid="_coordsys" >
		<!-- Usage 3: Child of COLLECTION, for ORM selection -->
			
			<!-- Test Case 5.29: no PRIMARY_KEY; must have PRIMARY_KEY in this context -->
			<!-- * each child must have at least 1 (all must have the same set in practice) -->
			<INSTANCE dmid="_timesys"  dmtype="model:TimeSys" >
				<PRIMARY_KEY dmtype="ivoa:string" value="TCB"/>
			</INSTANCE>
			<INSTANCE dmid="_spacecys" dmtype="model:SpaceSys" />
			
		</COLLECTION>

	</GLOBALS >
</VODML>
