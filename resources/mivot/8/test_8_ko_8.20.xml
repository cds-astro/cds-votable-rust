<?xml version="1.0" encoding="UTF-8"?>

<VODML
	xmlns="http://www.ivoa.net/xml/mivot">

	<MODEL name="model" url="http://aaaaaa" />
	<GLOBALS>

		<INSTANCE dmid="_aaa" dmtype="model:thing">

            <!-- Test Case 8.20: COLLECTION of INSTANCE + JOIN (local and external instances) -->
            <COLLECTION dmrole="iiiiii">
                <INSTANCE dmtype="model:object" />
                <JOIN sourceref="_someTable" dmref="_extInst1"/>
                <JOIN sourceref="_someOtherTable" dmref="_extInst2"/>
                <INSTANCE dmtype="model:object" />
                <INSTANCE dmtype="model:object" />
            </COLLECTION>
			
		</INSTANCE>

		
	</GLOBALS>
</VODML>



